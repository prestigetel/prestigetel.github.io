.account_serv
	
	&:before
		content ''
		position absolute
		width 100%
		height 6px		
		z-index 2
		top -6px
		background url(../img/general/bah_2.png)
		
	background-color #2c3949
	color #f9f9f9
	padding-bottom 60px
		
	h2
		color #f9f9f9
			
	
	
	.section_content
		min-height 870px
		background url(../img/general/account_serv_bg.png) bottom  no-repeat
		padding 0
		
		.info_item_wrap
			
			&:nth-child(2)
				
				.info_item
					margin-top 80px
					
			&:nth-child(8)
				
				.info_item
					margin-top 215px
					
			&:nth-child(6)
				
				.info_item
					margin-left 65px			
					
			&:nth-child(1), &:nth-child(7)
				
				.info_item
					margin-left 80px
					
					
			&:nth-child(1), &:nth-child(3)
				
				.info_item
					margin-top 160px
					
			&:nth-child(4), &:nth-child(6)
				
				.info_item
					margin-top 100px
					
					
			&:nth-child(7), &:nth-child(9)
				
				.info_item
					margin-top 80px			
					
			&:nth-child(3), &:nth-child(9)
				
				.info_item
					margin-left -25px
					
					
		.info_item
			margin 0
			min-height 130px
			
			&:after
				display none
				
				
 	.account_serv_img_wrap
		width 134px
		height 134px
		background-color mainColor
		border-radius 10em
		text-align center
		position absolute
		top 86px
		left 130px
		
		img
			width 70px
			height 70px		
			margin-top 30px
		
	

		
		
		
		
		
		
			
			
		
		
		
		
		